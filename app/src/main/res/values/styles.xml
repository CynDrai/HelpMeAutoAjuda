<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">#000000</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Login">
        <item name="colorControlNormal">#ff0000</item>
        <item name="colorControlActivated">#707770</item>
        <item name="colorControlHighlight">@color/accent_material_light</item>
    </style>

    <style name="ColorText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#707770</item>
    </style>

    <style name="Color2Text" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#232623</item>
    </style>

    <style name="ColorWText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#ced6cb</item>
    </style>

    <style name="ColorallText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#333532</item>
    </style>
    #333532
</resources>
